# .env file used by load_dotenv to insert environment variables in the jupter environment
# copy example.env to .env
# Required for research agents with external search
TAVILY_API_KEY=your_tavily_api_key_here    # name = default, type = dev

# Required for model usage
ANTHROPIC_API_KEY=your_anthropic_api_key_here

OPENAI_API_KEY=your_openai_api_key_here    # description = langchain-deep-agents-from-scratch

# Optional: For evaluation and tracing
LANGSMITH_API_KEY=your_langsmith_api_key_here    # description = langchain-deep-agents-from-scratch
LANGSMITH_TRACING=true
LANGSMITH_PROJECT=deep-agents-from-scratch